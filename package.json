{
  "name": "AblFormatter",
  "displayName": "AblFormatter",
  "description": "TODO",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Formatters"
  ],
  "activationEvents": [
    "onLanguage:abl"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "abl",
        "aliases": [
          "OpenEdge",
          "OpenEdge ABL",
          "Progress OpenEdge"
        ],
        "extensions": [
          ".p",
          ".i",
          ".cls",
          ".w"
        ]
      }
    ],
    "commands": [
      {
        "command": "AblFormatter.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "ui-test": "extest setup-and-run './out/ui-test/*-test.js' -s ./.test_dir -e ./.ext_dir -c 1.84.2 -t stable -m 'src/ui-test/MyMochaOptions.ts'",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.3.1",
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "@types/vscode": "^1.84.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vscode/test-electron": "^2.3.6",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "web-tree-sitter": "0.20.8"
  }
}